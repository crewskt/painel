  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Centexus/Aglypto/issues"
  },
  "homepage": "https://github.com/Centexus/Aglypto#readme",
  "dependencies": {
    "axios": "^0.23.0",
    "browserify": "^17.0.0",
    "concurrently": "^6.3.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "technicalindicators": "^3.1.0",
    "telegraf": "^4.4.2",
    "watchify": "^4.0.0"
  }
