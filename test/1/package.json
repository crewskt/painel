{
  "name": "aglypto",
  "version": "1.0.0",
  "description": "A fully fledged signal bot for crypto",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev-compile1": "watchify ./public/js/home/app.js -o ./public/js/home/index.js",
    "dev-compile2": "watchify ./public/js/accumulation/app.js -o ./public/js/accumulation/index.js",
    "dev-start": "concurrently \"npm run dev-compile1\" \"npm run dev-compile2\" \"nodemon server.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Centexus/Aglypto.git"
  },
